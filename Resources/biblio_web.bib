@article{motiwaleNeuralNetworkFinite2024,
  title = {A Neural Network Finite Element Approach for High Speed Cardiac Mechanics Simulations},
  author = {Motiwale, Shruti and Zhang, Wenbo and Feldmeier, Reese and Sacks, Michael S.},
