  file = {/Users/daby/Documents/biblio/Zotero_data/storage/F3RNDEJB/Motiwale et al. - 2024 - A neural network finite element approach for high speed cardiac mechanics simulations.pdf;/Users/daby/Documents/biblio/Zotero_data/storage/5FQTPU6Q/S0045782524003165.html}
}

