@article{daltonPhysicsinformedGraphNeural2023,
  title = {Physics-Informed Graph Neural Network Emulation of Soft-Tissue Mechanics},
  author = {Dalton, David and Husmeier, Dirk and Gao, Hao},
  year = {2023},
